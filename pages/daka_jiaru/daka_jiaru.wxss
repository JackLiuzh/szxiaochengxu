/* pages/daka_detail/daka_detail.wxss */
.box {
  display: flex;
  flex-direction: column;
}
.box .header{
  display: flex;
  flex-direction: row;
  height: 220rpx;
  background-color: #FFFFFF;
  justify-content: flex-start;
  margin-top: 1rpx;
  padding: 0 30rpx;
}
.box .header .header-image{
  margin-top: 10rpx;
  width:180rpx;
  height:180rpx;
}
.box .header .sub-hezi{
    display: flex;
    flex-direction: column;
    margin-left:26rpx;
    
    /* justify-content: space-around; */
}
.box .header .sub-hezi .main-title{
   color:#333;
   font-size: 34rpx;
   font-weight: bold;
   line-height: 74rpx;
}
.box .header .sub-hezi .sub-title{
  color:#999999;
  font-size:24rpx;
  font-weight: normal;
  line-height: 40rpx;
}
.box .header .sub-hezi .heizi-but{
    /* display: flex; */
    /* flex-direction: row; */
    /* justify-content: flex-start; */
    margin-top: 12rpx;
   
}
.box .header .sub-hezi .heizi-but button{
    margin-left:4rpx;
    width:150rpx;
    height:54rpx;
    border:2rpx solid #EA5959;
    font-size:26rpx;
    line-height:54rpx;
    border-radius: 30rpx;
    color:#EA5959;
    
} 
.box .header .sub-hezi .heizi-but button:nth-child(1){
   margin-right:30rpx;
   background-image: url("data:image/png;base64,")  ;
background-repeat: no-repeat;
background-size:30rpx 30rpx;
background-position:20rpx 10rpx;
padding-left:58rpx;
}
.box .header .sub-hezi .heizi-but button:nth-child(2){
   margin-right:30rpx;
   background-image: url("data:image/png;base64,")  ;
background-repeat: no-repeat;
background-size:30rpx 30rpx;
background-position:20rpx 10rpx;
padding-left:58rpx;
}
.box .header .sub-hezi .heizi-but button:nth-child(2){
   
}
.box .body{
   display: flex;
   flex-direction: column;
   margin-top:20rpx;
   /* background-color:#fff; */
   padding:0rpx 30rpx;
}
.box .body .bd-content{
    background-color:#fff;
    padding:20rpx;
    border-radius:10rpx;
    /* padding-bottom:42rpx; */
}
.box .body .bd-content .co-title{
    display: flex;
    flex-direction: row;
}
.box .body .bd-content .co-title .title-1{
    flex:1;
}
.box .body .bd-content .co-title text{
     font-size:30rpx;
}   
.box .body .bd-content .co-title text:nth-child(1){
     font-weight: bold;
     color:#333;
}   
.box .body .bd-content .co-title text:nth-child(2){
     color:#999;
}
.box .body .bd-content .co-image {
    position: relative;
    height: 325rpx;
}
.box .body .bd-content .co-image .im-words{
  position: absolute;
  z-index: 12;
  color: #fff;
  font-size:36rpx;
  padding:30rpx;
 
}
.box .body .bd-content .co-image .im-words button{
  width:190rpx;
  height:64rpx;
  border-radius:40rpx;
  background-color:#EA5959;
  color:#fff;
  line-height:64rpx;
  position:relative;
  left:-143rpx;
  bottom:-146rpx;
  padding:0;
  font-size:30rpx;

}
.box .body .bd-content .co-image image{
   position: absolute;
   top: 20rpx;
   width:100%;
   height:100%;
   opacity: .9;
   /* z-index: -1; */
   
}
.box .footer{
  display: flex;
  flex-direction: column;
  margin-top:20rpx;
}
.box .footer .footer-title{
  font-size: 40rpx;
  color:#333;
  font-weight: bold;
  /* padding:  30rpx; */
  background-color:#fff;
  padding-left:20rpx;
  padding-top:20rpx;

}
.box .footer .item-box{
  /* margin-bottom: 126rpx; */
}
.box .footer .item-box .item{
   display: flex;
   flex-direction: column;
   margin-bottom:2rpx;
   background-color:#fff;
}
.box .footer .item-box .item .item-cc {
   display: flex;
   flex-direction: row;
   padding:30rpx;
}
.box .footer .item-box .item .item-line{
   border-bottom:1rpx solid red;
}
.box .footer .item-box .item .item-cc image{

}
.box .footer .item-box .item .item-cc .item-content{
  display: flex;
  flex-direction: column;
  margin-left:23rpx;
}
.box .footer .item-box .item .item-cc .item-content .name{
  font-size:34rpx;
  color:#333;
  font-weight: bold;
  line-height:48rpx;
}
.box .footer .item-box .item .item-cc .item-content .time{
  font-size:24rpx;
  line-height:22px;
  color:#999;
}
 .box .footer .item-box .item .item-cc .item-content .words{
    font-size:34rpx;
    color:#333;
    line-height: 80rpx;
 }
 .box .footer .item-box .item .item-cc .item-content .sign{
    font-size:24rpx;
    color:#666;
    line-height: 33rpx;
    padding:10rpx 20rpx;
    background-color: #F3F4F5;
    margin-top:30rpx;
 }
